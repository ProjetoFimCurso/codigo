<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="start"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".MainMenu" >

    <ImageButton
        android:id="@string/dbOTHER"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="22dp"
        android:background="@drawable/ib_other"
        android:onClick="showOtherLists"
        android:src="@drawable/ib_other"
        android:tag="@string/title_activity_other_list"
        tools:ignore="ContentDescription" />

    <ImageButton
        android:id="@string/dbMAYBE_LATER"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@string/dbINBOX"
        android:layout_marginTop="26dp"
        android:layout_toLeftOf="@string/dbINCUBATOR"
        android:background="@drawable/ic_action_maybe"
        android:onClick="listar"
        android:src="@drawable/ic_action_maybe"
        android:tag="@string/MAYBE_LATER"
        tools:ignore="ContentDescription" />

    <ImageButton
        android:id="@string/dbINBOX"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="15dp"
        android:layout_toLeftOf="@string/dbDELEGATED"
        android:background="@drawable/ic_action_inbox"
        android:onClick="listar"
        android:src="@drawable/ic_action_inbox"
        android:tag="@string/INBOX"
        tools:ignore="ContentDescription" />

    <ImageButton
        android:id="@string/dbNEXT_ACTIONS"
        style="@style/AppBaseTheme"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@string/dbDELEGATED"
        android:layout_alignTop="@string/dbMAYBE_LATER"
        android:background="@drawable/ic_action_next_action"
        android:onClick="listar"
        android:src="@drawable/ic_action_next_action"
        android:tag="@string/NEXT_ACTIONS"
        tools:ignore="ContentDescription" />

    <ImageButton
        android:id="@string/dbCALENDAR"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_toLeftOf="@string/dbINCUBATOR"
        android:background="@drawable/ic_action_agenda"
        android:onClick="listar_calendar"
        android:src="@drawable/ic_action_agenda"
        android:tag="@string/CALENDAR"
        tools:ignore="ContentDescription" />

    <ImageButton
        android:id="@string/dbDELEGATED"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@string/dbCALENDAR"
        android:layout_toRightOf="@string/dbOTHER"
        android:background="@drawable/ic_action_delegados"
        android:onClick="listar"
        android:src="@drawable/ic_action_delegados"
        android:tag="@string/DELEGATED"
        tools:ignore="ContentDescription" />

    <ImageButton
        android:id="@string/dbINCUBATOR"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@string/dbOTHER"
        android:layout_alignLeft="@string/dbOTHER"
        android:layout_marginBottom="34dp"
        android:background="@drawable/ic_action_incubar"
        android:onClick="listar"
        android:src="@drawable/ic_action_incubar"
        android:tag="@string/INCUBATOR"
        tools:ignore="ContentDescription" />

</RelativeLayout>
