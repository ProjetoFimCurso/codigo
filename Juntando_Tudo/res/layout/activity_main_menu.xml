<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="start"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".MainMenu" >

    <ImageButton
        android:id="@+id/btIncubar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/btOther"
        android:layout_alignParentLeft="true"
        android:layout_marginBottom="29dp"
        android:layout_marginLeft="20dp"
        android:onClick="listar"
        android:src="@drawable/ic_action_incubar" />

    <ImageButton
        android:id="@+id/btAgenda"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="12dp"
        android:layout_marginTop="47dp"
        android:onClick="listar"
        android:src="@drawable/ic_action_agenda" />

    <ImageButton
        android:id="@+id/btNextAction"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/btAgenda"
        android:layout_marginRight="25dp"
        android:onClick="listar"
        android:src="@drawable/ic_action_next_action" />

    <ImageButton
        android:id="@+id/btTalvez"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/btNextAction"
        android:layout_alignTop="@+id/btIncubar"
        android:onClick="listar"
        android:src="@drawable/ic_action_maybe" />

    <ImageButton
        android:id="@+id/btOther"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="22dp"
        android:onClick="listar"
        android:src="@drawable/ib_other" />

    <ImageButton
        android:id="@+id/btInbox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/btDelegados"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="84dp"
        android:onClick="listar"
        android:src="@drawable/ic_action_inbox" />

    <ImageButton
        android:id="@+id/btDelegados"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/btInbox"
        android:layout_alignTop="@+id/btIncubar"
        android:onClick="listar"
        android:src="@drawable/ic_action_delegados" />

</RelativeLayout>
